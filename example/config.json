{
  "project_name": "example_project",
  "source_folders": [
    "example/source",
    "example/source"
  ],
  "output_dir": "./output",
  "model_output_path": "model.json",
  "recursive_search": true,
  "include_depth": 10,
  "file_filters": {
    "include": [],
    "exclude": []
  },
  "element_filters": {
    "structs": {
      "include": [],
      "exclude": []
    },
    "functions": {
      "include": [],
      "exclude": []
    },
    "enums": {
      "include": [],
      "exclude": []
    },
    "unions": {
      "include": [],
      "exclude": []
    },
    "macros": {
      "include": [],
      "exclude": []
    },
    "aliases": {
      "include": [],
      "exclude": []
    }
  },
  "include_filters": {
    "sample.c": [
      "^stdio\\.h$",
      "^stdlib\\.h$",
      "^string\\.h$",
      "^sample\\.h$",
      "^math_utils\\.h$",
      "^logger\\.h$",
      "^geometry\\.h$"
    ],
    "utils.c": [
      "^math\\.h$",
      "^time\\.h$"
    ]
  },
  "transformations": {
    "remove": {
      "structs": [],
      "functions": [],
      "macros": []
    },
    "rename": {
      "structs": {},
      "functions": {}
    }
  }
}
